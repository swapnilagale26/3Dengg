<XMLDB xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" PATH="local/attendance/db" VERSION="2022021000" COMMENT="XMLDB file for Moodle local/attendance" xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd">
