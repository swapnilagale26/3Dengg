<XMLDB xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" PATH="local/batch/db" VERSION="2022020300" COMMENT="XMLDB file for Moodle local/department" xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd">
